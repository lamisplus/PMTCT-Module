rTestedPositive}\n                                        onChange={handleInputChange} \n                                        style={{border: \"1px solid #014D88\", borderRadius:\"0.2rem\"}}\n                                    >\n                                        <option value={\"\"}></option>\n                                        {consent.map((value) => (\n                                            <option key={value.id} value={value.id}>\n                                                {value.display}\n                                            </option>\n                                        ))}\n                                        \n                                    </select>\n                                    \n                                </FormGroup>\n                            </div>\n                            <div className=\"form-group  col-md-4\">\n                                <FormGroup>\n                                    <Label>Has this partner ever hit, kick, slapped or otherwise physical hurt you? *</Label>\n                                    <select\n                                        className=\"form-control\"\n                                        name=\"physicalHurt\"\n                                        id=\"physicalHurt\"\n                                        value={objValues.physicalHurt}\n                                        onChange={handleInputChange} \n                                        style={{border: \"1px solid #014D88\", borderRadius:\"0.2rem\"}}\n                                    >\n                                         <option value={\"\"}></option>\n                                        {consent.map((value) => (\n                                            <option key={value.id} value={value.id}>\n                                                {value.display}\n                                            </option>\n                                        ))}\n                                        \n                                    </select>\n                                    \n                                </FormGroup>\n                            </div>\n                            <div className=\"form-group  col-md-4\">\n                                <FormGroup>\n                                    <Label>Has this partner ever threaten to hurt you? *</Label>\n                                    <select\n                                        className=\"form-control\"\n                                        name=\"threatenToHurt\"\n                                        id=\"threatenToHurt\"\n                                        value={objValues.threatenToHurt}\n                                        onChange={handleInputChange}\n                                        style={{border: \"1px solid #014D88\", borderRadius:\"0.2rem\"}}\n                                    >\n                                        <option value={\"\"}></option>\n                                        {consent.map((value) => (\n                                            <option key={value.id} value={value.id}>\n                                                {value.display}\n                                            </option>\n                                        ))}\n                                    </select>\n                                    \n                                </FormGroup>\n                            </div>\n                           \n                            <div className=\"form-group  col-md-4\">\n                                <FormGroup>\n                                    <Label>Has this partner ever threaten force you to do something sexually that made you uncomfortable ?  *</Label>\n                                    <select\n                                        className=\"form-control\"\n                                        name=\"sexuallyUncomfortable\"\n                    